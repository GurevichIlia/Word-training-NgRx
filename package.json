{
  "name": "word_training_nodejs",
  "version": "1.0.0",
  "description": "nodejs_api",
  "main": "index.js",
  "engines": {
    "node": "12.15.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "server": "nodemon src/index.ts",
    "client-install": "npm install --prefix client",
    "build": "tsc",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build && npm run client-install && npm run build --prefix client"
  },
  "author": "Ilia Gurevich",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongoose": "^5.5.43",
    "@types/morgan": "^1.7.37",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.7",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.4"
  }
}